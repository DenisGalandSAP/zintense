{
    "welcomeFile": "/$api-docs/odata/v4/intense/",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/\\$api-docs/?$",
            "localDir": ".",
            "target": "swagger-redirect.html",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/(.*)$",
            "target": "/$1",
            "destination": "srv-api",
            "authenticationType": "xsuaa",
            "csrfProtection": true
        }
    ]
}